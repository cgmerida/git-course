  "bugs": {
    "url": "https://github.com/cgmerida/git-course/issues"
  },
  "homepage": "https://github.com/cgmerida/git-course#readme",
  "devDependencies": {
    "lite-server": "^2.5.3",
    "node-sass": "^4.12.0",
    "onchange": "^6.0.0",
    "parallelshell": "^3.0.2",
    "sass": "^1.21.0"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.0"
  }
}
